<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="property.myController">
   <center>
      <FlowPane hgap="20.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button defaultButton="true" lineSpacing="2.0" mnemonicParsing="false" onAction="#onClickedPropertyListButton" prefHeight="120.0" prefWidth="126.0" text="View Properties" textAlignment="CENTER" textFill="#030428">
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin>
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor>
               <opaqueInsets>
                  <Insets top="5.0" />
               </opaqueInsets>
            </Button>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#onClickedCustomerListButton" prefHeight="120.0" prefWidth="134.0" text="View Customers" textFill="#0d0b75" />
            <Button defaultButton="true" mnemonicParsing="false" onAction="#onClickedPlaceOfInterestListButton" prefHeight="120.0" prefWidth="134.0" text="Place Of Interest" textFill="#0d0b75" />
            <Button defaultButton="true" lineSpacing="20.0" mnemonicParsing="false" onAction="#onClickedRentedPropertyListButton" prefHeight="120.0" prefWidth="134.0" text="Rented Properties" textFill="#0d0b75">
               <font>
                  <Font size="12.0" />
               </font>
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin>
            </Button>
            <Button defaultButton="true" lineSpacing="20.0" mnemonicParsing="false" onAction="#onClickedInvoiceListButton" prefHeight="120.0" prefWidth="134.0" text="Invoices" textFill="#0d0b75">
               <font>
                  <Font size="12.0" />
               </font>
               <FlowPane.margin>
                  <Insets top="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </center>
</BorderPane>
