<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="property.myController">
   <center>
      <FlowPane hgap="20.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button defaultButton="true" lineSpacing="2.0" mnemonicParsing="false" onAction="#onClickedPropertyListButton" prefHeight="120.0" prefWidth="120.0" text="View Properties" textAlignment="CENTER" textFill="#030428">
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin>
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor>
               <opaqueInsets>
                  <Insets top="5.0" />
               </opaqueInsets>
            </Button>
            <Button defaultButton="true" mnemonicParsing="false" prefHeight="120.0" prefWidth="120.0" text="View User" textFill="#0d0b75" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </center>
</BorderPane>
